{
    "name": "kalexhaym/laravel-telegram-bot",
    "description": "Telegram Bot package for Laravel",
    "keywords": [
        "laravel",
        "telegram",
        "bot"
    ],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kalexhaym\\LaravelTelegramBot\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aleksei Kovalenko",
            "email": "kalexhaym@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*",
        "laravel/framework": "^10.0 || ^11.0",
        "guzzlehttp/guzzle": "^7.8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "orchestra/testbench": "^8.0 || ^9.0",
        "orchestra/testbench-core": "^8.0 || ^9.0",
        "laravel/pint": "^1.14"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalexhaym\\LaravelTelegramBot\\TelegramServiceProvider"
            ]
        }
    }
}
